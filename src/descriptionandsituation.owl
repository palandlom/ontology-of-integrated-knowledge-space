    </owl:Class>
    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/situation.owl#Situation -->

    <owl:Class rdf:about="&situation;Situation">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&descriptionandsituation;satisfies"/>
