    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/owl/owlapi/социальная_ответственность">
        <rdf:type rdf:resource="http://www.semanticweb.org/owl/owlapi/Полярная_деятельность"/>
        <as:Association rdf:resource="http://www.semanticweb.org/owl/owlapi/дрейф_льда"/>
        <as:Association rdf:resource="http://www.semanticweb.org/owl/owlapi/заинтересованная_сторона"/>
        <as:Association rdf:resource="http://www.semanticweb.org/owl/owlapi/фиардовый_берег"/>
        <as:Association rdf:resource="http://www.semanticweb.org/owl/owlapi/этичное_поведение"/>
